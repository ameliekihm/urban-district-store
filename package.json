{
  "name": "shoppy",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.9.4",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.873.0",
    "@aws-sdk/client-ssm": "^3.873.0",
    "@aws-sdk/credential-providers": "^3.873.0",
    "@aws-sdk/lib-dynamodb": "^3.873.0",
    "@firebase/app": "^0.10.8",
    "@tanstack/react-query": "^5.51.21",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^1.11.0",
    "firebase": "^10.12.5",
    "jwt-decode": "^4.0.0",
    "oidc-client-ts": "^3.3.0",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-oidc-context": "^3.3.0",
    "react-router-dom": "^6.26.0",
    "react-scripts": "5.0.1",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "cp public/env.local.js public/env.js && react-scripts start",
    "build": "react-scripts build",
    "build:dev": "cp public/env.development.js public/env.js && react-scripts build",
    "build:staging": "cp public/env.staging.js public/env.js && react-scripts build",
    "build:prod": "cp public/env.production.js public/env.js && react-scripts build",
    "deploy:dev": "yarn build:dev && aws s3 sync build/ s3://urban-district-dev/ --delete && aws cloudfront create-invalidation --distribution-id E2ND9CQFOBAJ32 --paths '/*' && open https://dev.urban-district.click",
    "deploy:staging": "yarn build:staging && aws s3 sync build/ s3://urban-district-staging/ --delete && aws cloudfront create-invalidation --distribution-id E1BEU6MYHHLN3W --paths '/*' && open https://staging.urban-district.click",
    "deploy:prod": "yarn build:prod && aws s3 sync build/ s3://urban-district.click/ --delete && aws cloudfront create-invalidation --distribution-id E1JEBAR6H9BLMF --paths '/*' && open https://urban-district.click",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "tailwindcss": "^3.4.7"
  }
}
